<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.clientlist">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_start"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_start"
        android:supportsRtl="true"
        android:theme="@style/Theme.ClientList">
        <activity
            android:name=".LogoActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.ClientList.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" android:theme="@style/Theme.ClientList.NoActionBar" />
        <activity android:name=".editAct" />
        <activity android:name=".Settings.SettingsActivity" />

    </application>

</manifest>